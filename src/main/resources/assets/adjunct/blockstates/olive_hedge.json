{
  "multipart": [
    {
      "when": {
        "extend": "false"
      },
      "apply": {
        "model": "adjunct:block/olive_hedge_post"
      }
    },
    {
      "when": {
        "extend": "true"
      },
      "apply": {
        "model": "adjunct:block/olive_hedge_extend"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "adjunct:block/olive_hedge_side",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "adjunct:block/olive_hedge_side",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "adjunct:block/olive_hedge_side",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "adjunct:block/olive_hedge_side",
        "y": 270,
        "uvlock": true
      }
    }
  ]
}
